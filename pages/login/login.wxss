

.container {
  height: 100%;
  width: 100%;
  background-color: #fdb830;
  position: absolute;
}

.text-img {
  position: absolute;
  height: 112rpx;
  width: 465rpx;
  top: 317rpx;
  left: 136rpx;
}

.mountain-img {
  position: absolute;
  bottom: 0rpx;
  width: 100%;
  height: 386rpx;
}

.star-img-1 {
  position: absolute;
  height: 31rpx;
  width: 31rpx;
  top: 77rpx;
}

.yinfu-img { 
  position: absolute;
  top: 519rpx; 
  animation: yinfu-action 10s ease-in-out alternate infinite; 
  width: 39rpx;
  height: 64rpx; 
  left: 348rpx;
}

@keyframes yinfu-action {
  0% {transform: rotate(50deg); transform-origin: top center; }
  100% {transform: rotate(-50deg); transform-origin: top center;} 
}

.circle-img {
  position: absolute;
  top: 489rpx;
  height: 141rpx;
  width: 136rpx;
  animation: circle-action 10s linear infinite;
  left: 302rpx;  
}

@keyframes circle-action {
  100% {transform: rotate(360deg)}
}

